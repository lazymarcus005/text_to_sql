# Gemini (Google AI Studio)
GOOGLE_API_KEY=xxxxx
GEMINI_MODEL=gemini-2.5-flash-lite
GEMINI_TEMPERATURE=0.0

# DB (compose service name: db)
POSTGRES_HOST=db
POSTGRES_PORT=5432
POSTGRES_DB=appdb
POSTGRES_USER=app
POSTGRES_PASSWORD=app_pw

# Safety
SQL_STATEMENT_TIMEOUT_MS=5000
SQL_MAX_ROWS=200

# Text-to-SQL retries
TEXT2SQL_MAX_RETRIES=3
